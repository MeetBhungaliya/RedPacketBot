{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "build": {
    "env": {
      "TS_NODE_DEV": "true"
    },
    "commands": ["tsc --project tsconfig.json"]
  }
}
